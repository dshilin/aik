<div class='content'>
Текст на странице Контакты.

<% form_for(@nsar) do |f| %>
  <%= f.error_messages %>
  <p><%= label_tag 'Изменить текст на странице О нас' %><br/>
  	<%= f.text_area(:description, :size => "120x10") %>
  </p>
  <p>
    <%= f.submit "Изменить" %>
  </p>
<% end %>

<%= link_to 'Назад', nsars_path %>
</div>